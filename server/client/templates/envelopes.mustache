{{#envelopes}}
    <div class="envelope">
        <h1>{{familyNames}}</h1>
        <svg class="barcode" id="barcode-{{id}}"></svg>
        {{#families}}
            <h4>{{name}}</h4>
            <ul class="family container">
                {{#members}}
                    <li class="member row">
                        <span class="name">
                            {{firstName}}:
                        </span>
                        <span class="sizes">
                            <span class="bootSize row">
                                Size {{bootSize}}
                            </span>
                            <span class="product row">
                                {{product}}
                            </span>
                        </span>
                    </li>
                {{/members}}
            </ul>
        {{/families}}
    </div>
{{/envelopes}}